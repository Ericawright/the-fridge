<div id="results">
  <%@query['matches'].each do |match|%>
  <br><br>
  <h3 class="recipe_name"><%=match['recipeName']%></h3>
  <br><br>
  <div class="image-results">
    <img width="300px" height="300px" src="<%=match['imageUrlsBySize']['90']%>"> 
  </div> 
  <br>
  <br>
  <div class="recipe-details">
    <p class="time">Cooking time: <%=match['totalTimeInSeconds']%></p>
    <p class="rating2">Rating: <%=match['rating']%><p>
      <li>Ingredients: <%=match['ingredients'].join(", x")%></li>
    </div>
    <%end%>




